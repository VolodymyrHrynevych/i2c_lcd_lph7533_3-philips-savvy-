void setup () {
   Serial.begin (9600); 
   Wire.begin ();                                       // Init i2c
   Wire.setClock (100000L);                             // I2C- Bus setup 100kHz
   
   init_LCD ();
}

void sendPack(uint8_t* pack, int l){

  Wire.beginTransmission (I2CadrLCD);    // capture pack # 72
  for (byte n=0; n<l; n++){
    Wire.write (pack[n]);
  }  
  Wire.endTransmission ();
  delay (20);
}

